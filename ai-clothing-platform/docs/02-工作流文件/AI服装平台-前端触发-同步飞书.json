          }
        ]
      ]
    },
    "下载场景图片": {
      "main": [
        [
          {
            "node": "【DeerAPI】AI图片生成",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "【DeerAPI】AI图片生成": {
      "main": [
        [
          {
            "node": "提取生成结果",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "提取生成结果": {
      "main": [
        [
          {
            "node": "【DeerAPI】上传结果图",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "【DeerAPI】上传结果图": {
      "main": [
        [
          {
            "node": "【飞书】创建记录",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "【飞书】创建记录": {
